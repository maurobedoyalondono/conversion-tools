{
    "name": "race-distance",
    "fields": [
      {
        "name": "raceCode",
        "csvName": "raceCode",
        "type": "native",
        "datatype": "string",
        "mandatory": true
      },
      {
        "name": "distance",
        "csvName": "Distance",
        "type": "native",
        "datatype": "number",
        "mandatory": true
      },
      {
        "name": "startTime",
        "csvName": "Start Time",
        "type": "native",
        "datatype": "string",
        "mandatory": false
      },
      {
        "name": "tags",
        "csvName": "Tags",
        "type": "custom",
        "parser": "parseTags",
        "regex": "^.+(:.+)?(\\|.+:.+)*$",
        "mandatory": false
      },      
      {
        "name": "price",
        "csvName": "Price",
        "type": "native",
        "datatype": "number",
        "mandatory": false
      },
      {
        "name": "toughness",
        "csvName": "Toughness_Level",
        "type": "custom",
        "regex": "^[A-Za-z]+$",
        "mandatory": false
      }
    ]
  }
  