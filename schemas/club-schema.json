{
    "name": "club",
    "fields": [
      {
        "name": "code",
        "csvName": "code",
        "type": "native",
        "datatype": "string",
        "mandatory": false
      },
      {
        "name": "image",
        "csvName": "image",
        "type": "native",
        "datatype": "string",
        "mandatory": false
      },
      {
        "name": "banner",
        "csvName": "banner",
        "type": "native",
        "datatype": "string",
        "mandatory": false
      },
      {
        "name": "name",
        "csvName": "name",
        "type": "native",
        "datatype": "string",
        "mandatory": true
      },
      {
        "name": "description",
        "csvName": "description",
        "type": "localizable",
        "datatype": "string",
        "mandatory": false,
        "parser": "parseLocalization",
        "localizationDetails": {
          "defaultLanguage": "en",
          "localizationField": "translations"
        }
      },
      {
        "name": "plus",
        "csvName": "plus",
        "type": "custom",
        "parser": "parseTags",
        "regex": "^.+(:.+)?(\\|.+:.+)*$",
        "mandatory": false
      },
      {
        "name": "minus",
        "csvName": "minus",
        "type": "custom",
        "parser": "parseTags",
        "regex": "^.+(:.+)?(\\|.+:.+)*$",
        "mandatory": false
      },
      {
        "name": "disciplines",
        "csvName": "Disciplines",
        "type": "custom",
        "parser": "parseTags",
        "regex": "^.+(:.+)?(\\|.+:.+)*$",
        "mandatory": false
      },
      {
        "name": "weeklyMeets",
        "csvName": "weeklyMeets",
        "type": "native",
        "datatype": "number",
        "mandatory": false
      },
      {
        "name": "clubSize",
        "csvName": "clubSize",
        "type": "native",
        "datatype": "number",
        "mandatory": false
      },
      {
        "name": "socialNetworks",
        "csvName": "socialNetworks",
        "type": "custom",
        "parser": "parseSocialNetworks",
        "regex": "^(?:(?:ig|web|fb)@[^|]+\\|?)+$"
      },
      {
        "name": "promotional",
        "csvName": "promotional",
        "type": "custom",
        "regex": "^(Yes|No)(\\|\\d+)?$",
        "parser": "parsePromotional"
      },      
      {
        "name": "locations",
        "type": "aggregate",
        "schema": "club-location",      
        "aggregateKey": {
          "main": "code",
          "foreign": "clubCode"
        }
      },{
        "name": "trainers",
        "type": "aggregate",
        "schema": "club-trainer",      
        "aggregateKey": {
          "main": "code",
          "foreign": "clubCode"
        }
      },
      {
        "name": "comments",
        "type": "aggregate",
        "schema": "club-comment",      
        "aggregateKey": {
          "main": "code",
          "foreign": "clubCode"
        }
      }
    ]
  }
  